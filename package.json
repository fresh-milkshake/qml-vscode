{
    "name": "qml-language-highlighting",
    "displayName": "QML Highlighting",
    "description": "Syntax highlighting for QML Language",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/qml.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/fresh-milkshake/qml-vscode.git"
    },
    "publisher": "fresh-milkshake",
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "QML",
                "aliases": [
                    "QML",
                    "qml",
                    "Qml"
                ],
                "extensions": [
                    ".qml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "QML",
                "scopeName": "source.qml",
                "path": "./syntaxes/qml.tmLanguage.json"
            }
        ]
    }
}